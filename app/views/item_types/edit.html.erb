<%= stylesheet_link_tag 'admin', media: 'all', 'data-turbolinks-track': 'reload' %>
<% if user_level_staff? %>
<div class="table mt-5">
  <%= form_tag("item_type_update", method: "post") do %>
  <%= label_tag(:type_name, "Type Name") %>
  <%=text_field_tag :type_name,  @itemType.type_name %>
  <%= label_tag(:type_description, "Type Description") %>
  <%=text_field_tag :type_description,  @itemType.type_description %>
  <%= label_tag(:type_active, "is type location?") %>
  <%= check_box_tag( :type_active, value = true, checked = @itemType.type_active) %>
  <%= hidden_field_tag(:id, @itemType.id) %>
  <%= submit_tag("update location") %>
  <% end %>
</div>
<% end %>