<p>A new item added generated</p>

<%= render template: "layouts/app_nav" %>

<table class="item-display">
    <tr>
        <th>Actions</th>
        <th>Item ID</th>
        <th>Date</th>
        <th>Time</th>
        <th>Found by</th>
        <th>Found at</th>
        <th>Type of item</th>
        <th>Item description</th>
    </tr>
    <tr>
        <td>
            <%= link_to("Delete", '#', :class => 'action show') %> <%= link_to("Edit", '#', :class => 'action edit') %>
        </td>
        <td><%= @items.itemDate %></td>
        <td><%= @items.itemFoundAt %></td>
        <td><%= @items.itemFoundBy %></td>
        <td><%= @items.locations[@items.itemLocation] || 'none' %></td>
        <td><%= @items.types[@items.itemType.to_i] || 'No type' %></td>
        <td><%= @items.itemDescription %></td>
    </tr>
</table>