<%= render template: "layouts/app_nav" %>
Similar items listed below

<h1>Items list</h1>

<table class="listing" summary="Claimed list">
    <tr class="header">
      <th>Actions </th>
      <th>Item ID</th>
      <th>Date</th>
      <th>Time</th>
      <th>Found by</th>
      <th>Found at </th>
      <th>Type of item</th>
      <th>Item description</th>
      <th>Item image</th>
    </tr>
    <% @items.each do |item| %>
    <tr>
      <td>
        <%= link_to("Show", '#', :class => 'action show') %>
        <%= link_to("Edit", '#', :class => 'action edit') %>
      </td>
      <td><%= item.itemDate %></td>
      <td><%= item.itemFoundAt %></td>
      <td><%= item.itemFoundBy %></td>
      <td><%= item.locations[item.itemLocation] || 'none' %></td>
      <td><%=item.itemType %></td>
      <td><%= item.itemDescription %></td>
      <td>
          <% if item.image.attached? %>
              <image class="preview_image" src="<%=(url_for(item.image)) %>">
          <% else %>
              No image 
          <% end %>
      </td>
    </tr>
    <% end %>
  </table>